    <uses-permission android:name="android.permission.INTERNET"/>

    <application

        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App193"
        tools:targetApi="31">
        <activity
            android:name=".RestorePass"
            android:exported="false" />
        <activity
            android:name=".ButtonNavigation.Home"
            android:exported="false"></activity>
        <activity
            android:name=".Authentication.Register"
            android:exported="false"></activity>
        <activity
            android:name=".Authentication.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Intents.SignUp"
            android:exported="false"></activity>
        <activity
            android:name=".Intents.MainActivity"
            android:exported="false"></activity>
        <activity
            android:name=".Authentication.RestorePass"
            android:exported="false"></activity>
    </application>

</manifest>